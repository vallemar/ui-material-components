{
    "name": "@nativescript-community/ui-material-textview",
    "version": "7.0.34",
    "description": "Material Design Text views allow users to input text into your app.",
    "main": "./textview",
    "sideEffects": false,
    "typings": "./textview.d.ts",
    "scripts": {
        "tsc": "cpy ../../src/textview/textview.d.ts ./ && tsc -d",
        "tsc-win": "cpy ..\\..\\src\\textview\\textview.d.ts .\\ && tsc -d",
        "build": "npm run tsc",
        "build.watch": "npm run tsc -- -w",
        "build.win": "npm run tsc-win",
        "build.all": "npm run build && npm run build.angular",
        "build.all.win": "npm run build.win && npm run build.angular.win",
        "build.angular": "ng-packagr -p ../../src/textview/angular/ng-package.json -c ../../src/textview/angular/tsconfig.json",
        "build.angular.win": "ng-packagr -p ..\\..\\src\\textview\\angular\\package.json -c ..\\..\\src\\textview\\angular\\tsconfig.json",
        "clean": "rimraf ./*.d.ts ./*.js ./*.js.map"
    },
    "nativescript": {
        "platforms": {
            "android": "6.2.0",
            "ios": "6.2.0"
        }
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS",
        "Vue",
        "Angular",
        "preview|https://raw.githubusercontent.com/nativescript-community/ui-material-components/master/images/demo-textview.png"
    ],
    "author": {
        "name": "Martin Guillon",
        "email": "martin@akylas.fr"
    },
    "bugs": {
        "url": "https://github.com/nativescript-community/ui-material-components/issues"
    },
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/nativescript-community/ui-material-components/tree/master/packages/textview"
    },
    "readmeFilename": "README.md",
    "dependencies": {
        "@nativescript-community/text": "^1.5.2",
        "@nativescript-community/ui-material-core": "^7.0.34"
    },
    "gitHead": "240457b7952c4f0258aabc46615de63d1473ead1"
}
